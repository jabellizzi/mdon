{
  "name": "mdon",
  "version": "1.0.0",
  "description": "Markdown to json converter",
  "main": "index.js",
  "scripts": {
    "build-node": "webpack --config webpack.mdon-config.js",
    "build-web": "webpack --config webpack.web-config.js",
    "watch-web": "webpack --config webpack.web-config.js --watch",
    "serve": "webpack-dev-server --config webpack.web-config.js --open",
    "start": "npm run watch-web & npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jabellizzi/mdon.git"
  },
  "keywords": [
    "markdown",
    "json"
  ],
  "author": "John Bellizzi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jabellizzi/mdon/issues"
  },
  "homepage": "https://github.com/jabellizzi/mdon#readme",
  "dependencies": {
    "fs": "0.0.1-security",
    "markdown-it": "^8.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
